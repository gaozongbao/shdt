  </resultMap>


  <select id="getAllCellInfo" resultType="Map" >
    SELECT f.id,f.cellname,f.longitude,f.latitude,f.azimuth,f.coveragetype FROM fourg_gc f
    where 1=1
    <if test="gridid!=null">
      and f.grid =#{gridid}
    </if>
  </select>
  <select id="getAllCellInfoById" resultType="Map" >
    SELECT * FROM fourg_gc f
    where f.id =#{id}
  </select>

  <select id="getAllSanGe" resultType="map">
    SELECT g.id `value` ,g.gridid `name`,g.latitude,g.longitude,g.boundaries FROM grid g
  </select>

</mapper>