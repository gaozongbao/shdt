<mapper namespace="com.c.dao.FourgGcMapper">
  <resultMap id="BaseResultMap" type="com.c.model.FourgGc">
    <!--
      WARNING - @mbg.generated
    -->
    <result column="id" jdbcType="INTEGER" property="id" />
